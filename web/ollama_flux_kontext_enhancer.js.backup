    // æ›´å¼ºå¥çš„äº‹ä»¶ç»‘å®š
    function bindWidgetCallback(widget, widgetName) {
        if (!widget) {
            console.warn(`âš ï¸ Ollamaç‰ˆæœ¬${widgetName} widgetä¸ºç©ºï¼Œè·³è¿‡ç»‘å®š`);
