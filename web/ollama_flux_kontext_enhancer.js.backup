        }
    }
    
    // ç»‘å®šäº‹ä»¶
    bindWidgetCallback(guidanceStyleWidget, "guidance_style");
    bindWidgetCallback(guidanceTemplateWidget, "guidance_template");
    
    // å»¶è¿Ÿåˆå§‹åŒ–ï¼Œç¡®ä¿æ‰€æœ‰widgetséƒ½å·²å®Œå…¨åŠ è½½
    setTimeout(() => {
